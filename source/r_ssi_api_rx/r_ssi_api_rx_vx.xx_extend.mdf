<module xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" schemaVersion="2.0" xs:noNamespaceSchemaLocation="schema_mdf_2.00.xsd">
    <fileVersion>1.00</fileVersion>
    <formatVersion>2.00</formatVersion>

    <fileTemplate></fileTemplate>
    <configuration>
        <property id="SSI_STANDARD_CONFIG" display="Use Standard configuration" default="" keyword="#define SSI_STANDARD_CONFIG" type="checkbox">
            <option id="SSI_STANDARD_CONFIG.#define SSI_USER_UNIQUE_CONFIG      //" display="User unique" value="#define SSI_USER_UNIQUE_CONFIG      //"/>
            <option id="SSI_STANDARD_CONFIG." display="Standard" value=""/>
            <description>This SSI module support Standatrd configuration to be easy for users to configure SSI peripheral. However some users woulkd like to configure on their own way. Set SSI_STANDARD_CONFIG or SSI_USER_UNIQUE_CONFIG to definition below according to the requirement.
Standard = SSI_STANDARD_CONFIG
User unique = SSI_USER_UNIQUE_CONFIG</description>
        </property>
        <property id="SSI_CH0_IO_MODE" display="Ch0 Transmit / Receive mode" default="2u" type="combobox">
            <option id="SSI_CH0_IO_MODE.0u" display="SSI not to use" value="0u"/>
            <option id="SSI_CH0_IO_MODE.1u" display="SSI to Receive data" value="1u"/>
            <option id="SSI_CH0_IO_MODE.2u" display="SSI to Transmit data" value="2u"/>
            <option id="SSI_CH0_IO_MODE.3u" display="SSI to Receive and Transmit" value="3u"/>
            <description>Selects transmit / receive mode (I/O mode).
NOTE: This is a configuration which must be set wether in case of "SSI_STANDARD_CONFIG" or "SSI_USER_UNIQUE_CONFIG". </description>
        </property>
        <property id="SSI_CH1_IO_MODE" display="Ch1 Transmit / Receive mode" default="0u" type="combobox">
            <option id="SSI_CH1_IO_MODE.0u" display="SSI not to use" value="0u"/>
            <option id="SSI_CH1_IO_MODE.1u" display="SSI to Receive data" value="1u"/>
            <option id="SSI_CH1_IO_MODE.2u" display="SSI to Transmit data" value="2u"/>
            <option id="SSI_CH1_IO_MODE.3u" display="SSI to Receive and Transmit" value="3u"/>
            <description>Selects transmit / receive mode (I/O mode).
NOTE: This is a configuration which must be set wether in case of "SSI_STANDARD_CONFIG" or "SSI_USER_UNIQUE_CONFIG". </description>
        </property>
        <property id="SSI_CH0_SERIAL_IF_FMT" display="Ch0 Audio serial interface format" default="0u" type="combobox">
            <option id="SSI_CH0_SERIAL_IF_FMT.0u" display="I2S" value="0u"/>
            <option id="SSI_CH0_SERIAL_IF_FMT.1u" display="Left-justified" value="1u"/>
            <option id="SSI_CH0_SERIAL_IF_FMT.2u" display="Right-justified" value="2u"/>
            <constraint display="This setting is only valid for SSI Standard configuration." actionOnFail="disable">"${SSI_STANDARD_CONFIG}" == ""</constraint>
            <description>Selects audio serial interface format.
This setting is only valid for "SSI_STANDARD_CONFIG".</description>
        </property>
        <property id="SSI_CH1_SERIAL_IF_FMT" display="Ch1 Audio serial interface format" default="0u" type="combobox">
            <option id="SSI_CH1_SERIAL_IF_FMT.0u" display="I2S" value="0u"/>
            <option id="SSI_CH1_SERIAL_IF_FMT.1u" display="Left-justified" value="1u"/>
            <option id="SSI_CH1_SERIAL_IF_FMT.2u" display="Right-justified" value="2u"/>
            <constraint display="This setting is only valid for SSI Standard configuration." actionOnFail="disable">"${SSI_STANDARD_CONFIG}" == ""</constraint>
            <description>Selects audio serial interface format.
This setting is only valid for "SSI_STANDARD_CONFIG".</description>
        </property>
        <property id="SSI_CH0_DATA_WIDTH" display="Ch0 PCM data width" default="16u" type="combobox">
            <option id="SSI_CH0_DATA_WIDTH.8u" display="8 bits" value="8u"/>
            <option id="SSI_CH0_DATA_WIDTH.16u" display="16 bits" value="16u"/>
            <option id="SSI_CH0_DATA_WIDTH.18u" display="18 bits" value="18u"/>
            <option id="SSI_CH0_DATA_WIDTH.20u" display="20 bits" value="20u"/>
            <option id="SSI_CH0_DATA_WIDTH.22u" display="22 bits" value="22u"/>
            <option id="SSI_CH0_DATA_WIDTH.24u" display="24 bits" value="24u"/>
            <constraint display="This setting is only valid for SSI Standard configuration." actionOnFail="disable">"${SSI_STANDARD_CONFIG}" == ""</constraint>
            <description>Selects PCM data width.
This setting is only valid for "SSI_STANDARD_CONFIG".</description>
        </property>
        <property id="SSI_CH1_DATA_WIDTH" display="Ch1 PCM data width" default="16u" type="combobox">
            <option id="SSI_CH1_DATA_WIDTH.8u" display="8 bits" value="8u"/>
            <option id="SSI_CH1_DATA_WIDTH.16u" display="16 bits" value="16u"/>
            <option id="SSI_CH1_DATA_WIDTH.18u" display="18 bits" value="18u"/>
            <option id="SSI_CH1_DATA_WIDTH.20u" display="20 bits" value="20u"/>
            <option id="SSI_CH1_DATA_WIDTH.22u" display="22 bits" value="22u"/>
            <option id="SSI_CH1_DATA_WIDTH.24u" display="24 bits" value="24u"/>
            <constraint display="This setting is only valid for SSI Standard configuration." actionOnFail="disable">"${SSI_STANDARD_CONFIG}" == ""</constraint>
            <description>Selects PCM data width.
This setting is only valid for "SSI_STANDARD_CONFIG".</description>
        </property>
        <property id="SSI_CH0_BCLK" display="Ch0 Bit Clock" default="64u" type="combobox">
            <option id="SSI_CH0_BCLK.16u" display="16 fs" value="16u"/>
            <option id="SSI_CH0_BCLK.32u" display="32 fs" value="32u"/>
            <option id="SSI_CH0_BCLK.48u" display="48 fs" value="48u"/>
            <option id="SSI_CH0_BCLK.64u" display="64 fs" value="64u"/>
            <constraint display="This setting is only valid for SSI Standard configuration." actionOnFail="disable">"${SSI_STANDARD_CONFIG}" == ""</constraint>
            <constraint display="Master Clock must be the integral multiple of the Bit Clock." actionOnFail="error">"${SSI_MCLK}".replace(/[LUlu]+$/, '') % "${SSI_CH0_BCLK}".replace(/[LUlu]+$/, '') == 0</constraint>
            <description>Selects bit clock.
This setting is only valid for "SSI_STANDARD_CONFIG".</description>
        </property>
        <property id="SSI_CH1_BCLK" display="Ch1 Bit Clock" default="64u" type="combobox">
            <option id="SSI_CH1_BCLK.16u" display="16 fs" value="16u"/>
            <option id="SSI_CH1_BCLK.32u" display="32 fs" value="32u"/>
            <option id="SSI_CH1_BCLK.48u" display="48 fs" value="48u"/>
            <option id="SSI_CH1_BCLK.64u" display="64 fs" value="64u"/>
            <constraint display="This setting is only valid for SSI Standard configuration." actionOnFail="disable">"${SSI_STANDARD_CONFIG}" == ""</constraint>
            <constraint display="Master Clock must be the integral multiple of the Bit Clock." actionOnFail="error">"${SSI_MCLK}".replace(/[LUlu]+$/, '') % "${SSI_CH1_BCLK}".replace(/[LUlu]+$/, '') == 0</constraint>
            <description>Selects bit clock.
This setting is only valid for "SSI_STANDARD_CONFIG".</description>
        </property>
        <property id="SSI_MCLK" display="Master Clock" default="256u" type="textbox">
            <constraint display="This setting is only valid for SSI Standard configuration." actionOnFail="disable">"${SSI_STANDARD_CONFIG}" == ""</constraint>
            <constraint display="Value must be an integer" actionOnFail="error">testInteger("${SSI_MCLK}")</constraint>
            <constraint display="Value must be in the range 16 ~ 8192" actionOnFail="error">("${SSI_MCLK}".replace(/[LUlu]+$/, '') &gt;= 16) &amp;&amp; ("${SSI_MCLK}".replace(/[LUlu]+$/, '') &lt;= 8192)</constraint>
            <constraint display="Master Clock must be the integral multiple of the Bit Clock" actionOnFail="error">("${SSI_MCLK}".replace(/[LUlu]+$/, '') % "${SSI_CH0_BCLK}".replace(/[LUlu]+$/, '') == 0) &amp;&amp; ("${SSI_MCLK}".replace(/[LUlu]+$/, '') % "${SSI_CH1_BCLK}".replace(/[LUlu]+$/, '') == 0)</constraint>
            <description>Master clock frequency 
  This setting is only valid for "SSI_STANDARD_CONFIG".
  Set a value from following alternatives to SSIn_MCLK.
----------------------------------
 Clock               value
----------------------------------
  16Fs                (16u)
  32Fs                (32u)
   :                    :
  8192Fs              (8192u)
NOTE: Set the Master Clock to the integral multiple of the Bit Clock.
</description>
        </property>
        <property id="SSI_CH0_CLK_MODE" display="Ch0 Clock Supply Mode" default="0u" type="combobox">
            <option id="SSI_CH0_CLK_MODE.0u" display="Master mode" value="0u"/>
            <option id="SSI_CH0_CLK_MODE.1u" display="Slave mode" value="1u"/>
            <constraint display="This setting is only valid for standard configuration." actionOnFail="disable">"${SSI_STANDARD_CONFIG}" == ""</constraint>
            <description>Selects clock supply mode.
This setting is only valid for "SSI_STANDARD_CONFIG".</description>
        </property>
        <property id="SSI_CH1_CLK_MODE" display="Ch1 Clock Supply Mode" default="0u" type="combobox">
            <option id="SSI_CH1_CLK_MODE.0u" display="Master mode" value="0u"/>
            <option id="SSI_CH1_CLK_MODE.1u" display="Slave mode" value="1u"/>
            <constraint display="This setting is only valid for standard configuration." actionOnFail="disable">"${SSI_STANDARD_CONFIG}" == ""</constraint>
            <description>Selects clock supply mode.
This setting is only valid for "SSI_STANDARD_CONFIG".</description>
        </property>
        <property id="SSI_CH0_TTRG_NUMBER" display="Ch0 Tx Empty trigger" default="4u" type="combobox">
            <option id="SSI_CH0_TTRG_NUMBER.1u" display="1 (PCM data width = 18/20/22/24 bits)" value="1u"/>
            <option id="SSI_CH0_TTRG_NUMBER.2u" display="2 (PCM data width = 16/18/20/22/24 bits)" value="2u"/>
            <option id="SSI_CH0_TTRG_NUMBER.3u" display="3 (PCM data width = 18/20/22/24 bits)" value="3u"/>
            <option id="SSI_CH0_TTRG_NUMBER.4u" display="4 (PCM data width = 8/16 bits)" value="4u"/>
            <option id="SSI_CH0_TTRG_NUMBER.6u" display="6 (PCM data width = 16 bits only)" value="6u"/>
            <option id="SSI_CH0_TTRG_NUMBER.8u" display="8 (PCM data width = 8 bits only)" value="8u"/>
            <option id="SSI_CH0_TTRG_NUMBER.12u" display="12 (PCM data width = 8 bits only)" value="12u"/>
            <constraint display="This setting is only valid for standard configuration." actionOnFail="disable">"${SSI_STANDARD_CONFIG}" == ""</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH0_TTRG_NUMBER}" != "1u" || "${SSI_CH0_DATA_WIDTH}" == "18u" || "${SSI_CH0_DATA_WIDTH}" == "20u" || "${SSI_CH0_DATA_WIDTH}" == "22u" || "${SSI_CH0_DATA_WIDTH}" == "24u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH0_TTRG_NUMBER}" != "2u" || "${SSI_CH0_DATA_WIDTH}" == "16u" || "${SSI_CH0_DATA_WIDTH}" == "18u" || "${SSI_CH0_DATA_WIDTH}" == "20u" || "${SSI_CH0_DATA_WIDTH}" == "22u" || "${SSI_CH0_DATA_WIDTH}" == "24u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH0_TTRG_NUMBER}" != "3u" || "${SSI_CH0_DATA_WIDTH}" == "18u" || "${SSI_CH0_DATA_WIDTH}" == "20u" || "${SSI_CH0_DATA_WIDTH}" == "22u" || "${SSI_CH0_DATA_WIDTH}" == "24u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH0_TTRG_NUMBER}" != "4u" || "${SSI_CH0_DATA_WIDTH}" == "8u" || "${SSI_CH0_DATA_WIDTH}" == "16u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH0_TTRG_NUMBER}" != "6u" || "${SSI_CH0_DATA_WIDTH}" == "16u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH0_TTRG_NUMBER}" != "8u" || "${SSI_CH0_DATA_WIDTH}" == "8u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH0_TTRG_NUMBER}" != "12u" || "${SSI_CH0_DATA_WIDTH}" == "8u"</constraint>
            <description>Transmit Data Trigger number selection
  Set a value from following alternatives to SSIn_TTRG_NUMBER.
----------------------------------
 Data length           value
----------------------------------
   8bit                 (12u) or (8u) or (4u)
  16bit                 (6u)  or (4u) or (2u)
  18,20,22,24bit        (3u)  or (2u) or (1u)
This setting is only valid for "SSI_STANDARD_CONFIG".</description>
        </property>
        <property id="SSI_CH1_TTRG_NUMBER" display="Ch1 Tx Empty trigger" default="4u" type="combobox">
            <option id="SSI_CH1_TTRG_NUMBER.1u" display="1 (PCM data width = 18/20/22/24 bits)" value="1u"/>
            <option id="SSI_CH1_TTRG_NUMBER.2u" display="2 (PCM data width = 16/18/20/22/24 bits)" value="2u"/>
            <option id="SSI_CH1_TTRG_NUMBER.3u" display="3 (PCM data width = 18/20/22/24 bits)" value="3u"/>
            <option id="SSI_CH1_TTRG_NUMBER.4u" display="4 (PCM data width = 8/16 bits)" value="4u"/>
            <option id="SSI_CH1_TTRG_NUMBER.6u" display="6 (PCM data width = 16 bits only)" value="6u"/>
            <option id="SSI_CH1_TTRG_NUMBER.8u" display="8 (PCM data width = 8 bits only)" value="8u"/>
            <option id="SSI_CH1_TTRG_NUMBER.12u" display="12 (PCM data width = 8 bits only)" value="12u"/>
            <constraint display="This setting is only valid for standard configuration." actionOnFail="disable">"${SSI_STANDARD_CONFIG}" == ""</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH1_TTRG_NUMBER}" != "1u" || "${SSI_CH1_DATA_WIDTH}" == "18u" || "${SSI_CH1_DATA_WIDTH}" == "20u" || "${SSI_CH1_DATA_WIDTH}" == "22u" || "${SSI_CH1_DATA_WIDTH}" == "24u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH1_TTRG_NUMBER}" != "2u" || "${SSI_CH1_DATA_WIDTH}" == "16u" || "${SSI_CH1_DATA_WIDTH}" == "18u" || "${SSI_CH1_DATA_WIDTH}" == "20u" || "${SSI_CH1_DATA_WIDTH}" == "22u" || "${SSI_CH1_DATA_WIDTH}" == "24u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH1_TTRG_NUMBER}" != "3u" || "${SSI_CH1_DATA_WIDTH}" == "18u" || "${SSI_CH1_DATA_WIDTH}" == "20u" || "${SSI_CH1_DATA_WIDTH}" == "22u" || "${SSI_CH1_DATA_WIDTH}" == "24u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH1_TTRG_NUMBER}" != "4u" || "${SSI_CH1_DATA_WIDTH}" == "8u" || "${SSI_CH1_DATA_WIDTH}" == "16u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH1_TTRG_NUMBER}" != "6u" || "${SSI_CH1_DATA_WIDTH}" == "16u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH1_TTRG_NUMBER}" != "8u" || "${SSI_CH1_DATA_WIDTH}" == "8u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH1_TTRG_NUMBER}" != "12u" || "${SSI_CH1_DATA_WIDTH}" == "8u"</constraint>
            <description>Transmit Data Trigger number selection
  Set a value from following alternatives to SSIn_TTRG_NUMBER.
----------------------------------
 Data length           value
----------------------------------
   8bit                 (12u) or (8u) or (4u)
  16bit                 (6u)  or (4u) or (2u)
  18,20,22,24bit        (3u)  or (2u) or (1u)
This setting is only valid for "SSI_STANDARD_CONFIG".</description>
        </property>
        <property id="SSI_CH0_RTRG_NUMBER" display="Ch0 Rx Full trigger" default="4u" type="combobox">
            <option id="SSI_CH0_RTRG_NUMBER.1u" display="1 (PCM data width = 18/20/22/24 bits)" value="1u"/>
            <option id="SSI_CH0_RTRG_NUMBER.2u" display="2 (PCM data width = 16/18/20/22/24 bits)" value="2u"/>
            <option id="SSI_CH0_RTRG_NUMBER.3u" display="3 (PCM data width = 18/20/22/24 bits)" value="3u"/>
            <option id="SSI_CH0_RTRG_NUMBER.4u" display="4 (PCM data width = 8/16 bits)" value="4u"/>
            <option id="SSI_CH0_RTRG_NUMBER.6u" display="6 (PCM data width = 16 bits only)" value="6u"/>
            <option id="SSI_CH0_RTRG_NUMBER.8u" display="8 (PCM data width = 8 bits only)" value="8u"/>
            <option id="SSI_CH0_RTRG_NUMBER.12u" display="12 (PCM data width = 8 bits only)" value="12u"/>
            <constraint display="This setting is only valid for standard configuration." actionOnFail="disable">"${SSI_STANDARD_CONFIG}" == ""</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH0_RTRG_NUMBER}" != "1u" || "${SSI_CH0_DATA_WIDTH}" == "18u" || "${SSI_CH0_DATA_WIDTH}" == "20u" || "${SSI_CH0_DATA_WIDTH}" == "22u" || "${SSI_CH0_DATA_WIDTH}" == "24u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH0_RTRG_NUMBER}" != "2u" || "${SSI_CH0_DATA_WIDTH}" == "16u" || "${SSI_CH0_DATA_WIDTH}" == "18u" || "${SSI_CH0_DATA_WIDTH}" == "20u" || "${SSI_CH0_DATA_WIDTH}" == "22u" || "${SSI_CH0_DATA_WIDTH}" == "24u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH0_RTRG_NUMBER}" != "3u" || "${SSI_CH0_DATA_WIDTH}" == "18u" || "${SSI_CH0_DATA_WIDTH}" == "20u" || "${SSI_CH0_DATA_WIDTH}" == "22u" || "${SSI_CH0_DATA_WIDTH}" == "24u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH0_RTRG_NUMBER}" != "4u" || "${SSI_CH0_DATA_WIDTH}" == "8u" || "${SSI_CH0_DATA_WIDTH}" == "16u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH0_RTRG_NUMBER}" != "6u" || "${SSI_CH0_DATA_WIDTH}" == "16u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH0_RTRG_NUMBER}" != "8u" || "${SSI_CH0_DATA_WIDTH}" == "8u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH0_RTRG_NUMBER}" != "12u" || "${SSI_CH0_DATA_WIDTH}" == "8u"</constraint>
            <description>Receive Data Trigger number selection
  Set a value from following alternatives to SSIn_RTRG_NUMBER.
----------------------------------
 Data length           value
----------------------------------
   8bit                 (4u)  or (8u) or (12u)
  16bit                 (2u)  or (4u) or (6u)
  18,20,22,24bit        (1u)  or (2u) or (3u)
This setting is only valid for "SSI_STANDARD_CONFIG".</description>
        </property>
        <property id="SSI_CH1_RTRG_NUMBER" display="Ch1 Rx Full trigger" default="4u" type="combobox">
            <option id="SSI_CH1_RTRG_NUMBER.1u" display="1 (PCM data width = 18/20/22/24 bits)" value="1u"/>
            <option id="SSI_CH1_RTRG_NUMBER.2u" display="2 (PCM data width = 16/18/20/22/24 bits)" value="2u"/>
            <option id="SSI_CH1_RTRG_NUMBER.3u" display="3 (PCM data width = 18/20/22/24 bits)" value="3u"/>
            <option id="SSI_CH1_RTRG_NUMBER.4u" display="4 (PCM data width = 8/16 bits)" value="4u"/>
            <option id="SSI_CH1_RTRG_NUMBER.6u" display="6 (PCM data width = 16 bits only)" value="6u"/>
            <option id="SSI_CH1_RTRG_NUMBER.8u" display="8 (PCM data width = 8 bits only)" value="8u"/>
            <option id="SSI_CH1_RTRG_NUMBER.12u" display="12 (PCM data width = 8 bits only)" value="12u"/>
            <constraint display="This setting is only valid for standard configuration." actionOnFail="disable">"${SSI_STANDARD_CONFIG}" == ""</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH1_RTRG_NUMBER}" != "1u" || "${SSI_CH1_DATA_WIDTH}" == "18u" || "${SSI_CH1_DATA_WIDTH}" == "20u" || "${SSI_CH1_DATA_WIDTH}" == "22u" || "${SSI_CH1_DATA_WIDTH}" == "24u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH1_RTRG_NUMBER}" != "2u" || "${SSI_CH1_DATA_WIDTH}" == "16u" || "${SSI_CH1_DATA_WIDTH}" == "18u" || "${SSI_CH1_DATA_WIDTH}" == "20u" || "${SSI_CH1_DATA_WIDTH}" == "22u" || "${SSI_CH1_DATA_WIDTH}" == "24u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH1_RTRG_NUMBER}" != "3u" || "${SSI_CH1_DATA_WIDTH}" == "18u" || "${SSI_CH1_DATA_WIDTH}" == "20u" || "${SSI_CH1_DATA_WIDTH}" == "22u" || "${SSI_CH1_DATA_WIDTH}" == "24u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH1_RTRG_NUMBER}" != "4u" || "${SSI_CH1_DATA_WIDTH}" == "8u" || "${SSI_CH1_DATA_WIDTH}" == "16u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH1_RTRG_NUMBER}" != "6u" || "${SSI_CH1_DATA_WIDTH}" == "16u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH1_RTRG_NUMBER}" != "8u" || "${SSI_CH1_DATA_WIDTH}" == "8u"</constraint>
            <constraint display="Illegal value is selected" actionOnFail="error">"${SSI_CH1_RTRG_NUMBER}" != "12u" || "${SSI_CH1_DATA_WIDTH}" == "8u"</constraint>
            <description>Receive Data Trigger number selection
  Set a value from following alternatives to SSIn_RTRG_NUMBER.
----------------------------------
 Data length           value
----------------------------------
   8bit                 (4u)  or (8u) or (12u)
  16bit                 (2u)  or (4u) or (6u)
  18,20,22,24bit        (1u)  or (2u) or (3u)
This setting is only valid for "SSI_STANDARD_CONFIG".</description>
        </property>
        <property id="SSI_CH0_TTRG" display="SSI0.SSIFCR.TTRG[1:0] Bits" default="3u" type="combobox">
            <option id="SSI_CH0_TTRG.0u" display="7 (1)" value="0u"/>
            <option id="SSI_CH0_TTRG.1u" display="6 (2)" value="1u"/>
            <option id="SSI_CH0_TTRG.2u" display="4 (4)" value="2u"/>
            <option id="SSI_CH0_TTRG.3u" display="2 (6)" value="3u"/>
            <constraint display="This setting is only valid for user unique configuration." actionOnFail="disable">"${SSI_STANDARD_CONFIG}" != ""</constraint>
            <description>Transmit FIFO Threshold Setting
